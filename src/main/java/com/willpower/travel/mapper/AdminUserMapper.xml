<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.willpower.travel.mapper.AdminUserMapper">

<select id="findAdminUser" resultType="com.willpower.travel.pojo.AdminUser">
        select * from t_pz_admin_user where user_name=#{userName} and password=#{password} and delete_status=0
    </select>
</mapper>